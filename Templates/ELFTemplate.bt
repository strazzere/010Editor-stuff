string VAddr32(Elf32_Addr &addr) {
    local char buf[128];
    SPrintf(buf, "0x%08X", addr);
    return buf;
}

string VAddr64(Elf64_Addr &addr) {
    local char buf[128];
    SPrintf(buf, "0x%016LX", addr);
    return buf;
}

