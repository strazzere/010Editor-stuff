typedef struct {
    char file_identification[4];
    if(Strcmp(file_identification, "\x7FELF")) {
        PrintWarning("Invalid ELF file");
        return -1;
    }

    ei_class_2_e ei_class_2;
    ei_data_e ei_data;

    if(ei_data == ELFDATA2LSB) {
        LittleEndian();
    } else {
        BigEndian();
    }

    ei_version_e    ei_version;
    ei_osabi_e    ei_osabi;
    uchar    ei_abiversion;
    uchar    ei_pad[6];
    uchar    ei_nident_SIZE;
} e_ident_t;

